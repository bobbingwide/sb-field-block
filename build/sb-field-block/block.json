{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "oiksb/sb-field-block",
  "title": "Field block",
  "category": "text",
  "icon": "info-outline",
  "description": "Displays a post meta data field",
  "supports": {
    "html": false,
    "className": true,
    "customClassName": true,
    "align": true,
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "textAlign": true,
      "__experimentalTextDecoration": true,
      "__experimentalFontStyle": true,
      "__experimentalFontWeight": true,
      "__experimentalLetterSpacing": true,
      "__experimentalTextTransform": true
    },
    "color": {
      "background": true,
      "link": true,
      "text": true
    },
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": true
    },
    "shadow": true,
    "__experimentalBorder": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true,
      "__experimentalDefaultControls": {
        "color": true,
        "radius": true,
        "style": true,
        "width": true
      }
    }
  },
  "attributes": {
    "postType": {
      "type": "string"
    },
    "fieldName": {
      "type": "string"
    },
    "showLabel": {
      "type": "boolean",
      "default": true
    }
  },
  "usesContext": [
    "postType",
    "postId"
  ],
  "textdomain": "sb-field-block",
  "editorScript": "file:../../build/sb-field-block.js",
  "editorStyle": "file:../../build/sb-field-block.css",
  "style": "file:../../build/style-sb-field-block.css"
}